# Mobile Security Agent Configuration - Kubernetes Version

# MobSF Configuration - Use Kubernetes service URL as fallback
MOBSF_API_URL=http://mobsf-service:8000
# MOBSF_API_KEY will be set by Kubernetes secret

# AI Provider Configuration
AI_PROVIDER=groq
AI_MODEL_NAME=llama-3.3-70b-versatile
AI_TEMPERATURE=0.1
AI_MAX_TOKENS=2000

# Analysis Configuration
PARALLEL_SCANNING=true
MAX_CONCURRENT_SCANS=3
ANALYSIS_TIMEOUT=300

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=logs/agent.log

# Report Configuration
REPORT_OUTPUT_DIR=reports
REPORT_FORMAT=html